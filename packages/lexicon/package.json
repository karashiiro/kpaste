{
  "name": "@kpaste-app/lexicon",
  "version": "1.0.2",
  "description": "ATProto lexicon types for KPaste",
  "type": "module",
  "main": "./src/lexicons/index.ts",
  "types": "./src/lexicons/index.ts",
  "exports": {
    ".": "./src/lexicons/index.ts",
    "./types": "./src/lexicons/types/moe/karashiiro/kpaste/paste.ts"
  },
  "publishConfig": {
    "main": "./dist/index.js",
    "types": "./dist/index.d.ts",
    "exports": {
      ".": {
        "types": "./dist/index.d.ts",
        "import": "./dist/index.js"
      },
      "./types": {
        "types": "./dist/types/moe/karashiiro/kpaste/paste.d.ts",
        "import": "./dist/types/moe/karashiiro/kpaste/paste.js"
      }
    }
  },
  "files": [
    "dist",
    "LICENSE",
    "README.md"
  ],
  "scripts": {
    "generate": "lex-cli generate -c ./lex.config.js",
    "build": "tsc",
    "prepublishOnly": "pnpm build"
  },
  "keywords": [
    "atproto",
    "lexicon",
    "kpaste",
    "paste",
    "snippet",
    "bluesky"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/karashiiro/kpaste.git",
    "directory": "packages/lexicon"
  },
  "author": "karashiiro",
  "license": "MIT",
  "devDependencies": {
    "@atcute/lex-cli": "^2.2.0",
    "typescript": "^5.0.0"
  },
  "dependencies": {
    "@atcute/lexicons": "^1.2.2"
  }
}
